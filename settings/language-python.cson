'.source.python':
  'editor':
    'autoIndentOnPaste': false
    'softTabs': true
    'tabLength': 4
    'commentStart': '# '
    'increaseIndentPattern': '^\\s*(((class|def|elif|except|finally|for|if|try|with|while)\\b.*:.*)|(else\\b.*:\\s*))$'
    'decreaseIndentPattern': '^\\s*(elif|else|except|finally)\\b.*:\\s*$'
